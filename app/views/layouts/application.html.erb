<!DOCTYPE html>
<html>
<head>
  <title>MeetMe</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<nav>
    <section>
        <%= form_for Search.new do |f| %>
            <%= f.label "Search by name" %><%= f.text_field :query %>
            <%= f.submit "Search" %>
        <% end %>
    </section>
</nav>

<%= yield %>

</body>
</html>
